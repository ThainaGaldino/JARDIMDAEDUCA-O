<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/lilas"
    tools:context=".Video">

    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="186dp"
        android:layout_height="183dp"
        android:layout_marginBottom="104dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toTopOf="@+id/appCompatImageView13"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="102dp" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/appCompatImageView13"
        android:layout_width="397dp"
        android:layout_height="333dp"
        android:src="@drawable/margem"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="292dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textView_assistir"
        android:layout_width="290dp"
        android:layout_height="44dp"
        android:layout_marginStart="88dp"
        android:text="@string/text_assistir"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="italic|bold"
        app:layout_constraintStart_toStartOf="@+id/appCompatImageView13"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="322dp" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/appCompatImageView14"
        android:layout_width="291dp"
        android:layout_height="291dp"
        android:layout_marginStart="60dp"
        android:src="@drawable/video_corpo_humano"
        app:layout_constraintStart_toStartOf="@+id/appCompatImageView13"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="334dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_iniciaratividade"
        android:layout_width="160dp"
        android:layout_height="48dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/bg_button_normal"
        android:letterSpacing="0.04"
        android:text="@string/text_atividade"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appCompatImageView14"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>